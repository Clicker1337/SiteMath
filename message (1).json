{
    "files.associations": {
        "*.kit": "html",
        "*.scss": "scss",
        "*.css": "css"
    },
    "editor.minimap.enabled": false,
    "files.autoSave": "onFocusChange",
    "editor.detectIndentation": false,
    "editor.mouseWheelZoom": true,
    "liveSassCompile.settings.generateMap": false,  
    "liveSassCompile.settings.formats": [
        {
            "format": "compressed",
            "extensionName": ".css",
            "savePath": "/css"
        }
    ],
    "explorer.confirmDelete": false,
    "vscodeGoogleTranslate.preferredLanguage": "Chinese (Traditional)",
    "sync.gist": "c8f60114a1ce491ed2408283bb9435ce",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "[scss]": {
        "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
    },
    "explorer.confirmDragAndDrop": false,
    "path-autocomplete.pathMappings": {
        "@img": "${folder}/img",
        "@scss": "${folder}/src/scss",
        "@js": "${folder}/src/js",
        "@files": "${folder}/src/files",
        "@fonts": "${folder}/fonts"
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "workbench.startupEditor": "none",
    "editor.guides.bracketPairs": true,
    "--bracket-spacing": true,
    "workbench.colorTheme": "Darcula",
    "editor.accessibilitySupport": "off",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "workbench.colorCustomizations": {
        "terminal.background": "#161B1D",
        "terminal.foreground": "#7EA2B4",
        "terminalCursor.background": "#7EA2B4",
        "terminalCursor.foreground": "#7EA2B4",
        "terminal.ansiBlack": "#161B1D",
        "terminal.ansiBlue": "#257FAD",
        "terminal.ansiBrightBlack": "#5A7B8C",
        "terminal.ansiBrightBlue": "#257FAD",
        "terminal.ansiBrightCyan": "#2D8F6F",
        "terminal.ansiBrightGreen": "#568C3B",
        "terminal.ansiBrightMagenta": "#6B6BB8",
        "terminal.ansiBrightRed": "#D22D72",
        "terminal.ansiBrightWhite": "#EBF8FF",
        "terminal.ansiBrightYellow": "#8A8A0F",
        "terminal.ansiCyan": "#2D8F6F",
        "terminal.ansiGreen": "#568C3B",
        "terminal.ansiMagenta": "#6B6BB8",
        "terminal.ansiRed": "#D22D72",
        "terminal.ansiWhite": "#7EA2B4",
        "terminal.ansiYellow": "#8A8A0F"
    },
    "terminalThemes.style": "Atelier Dune Light (base16)",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 15,
    "editor.fontFamily": "Fira Code",
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontSize": 12,
    "git.autofetch": true,
    "gitlens.defaultTimeFormat": null,
    "terminal.integrated.smoothScrolling": true,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command",
        "push:command"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "line",
    "editor.cursorSmoothCaretAnimation": "on",
    "git.confirmSync": false,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "redhat.telemetry.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    //--------------------------PRETTIER--------------------------
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    // "prettier.useTabs": false,
    // "prettier.bracketSpacing": false,
    // "prettier.arrowParens": "always",
    // "[json]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[javascript]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[typescript]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[typescriptreact]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[css]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[html]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    // "[vue]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    //-------------------------------------------------------------
    "editor.formatOnSaveMode": "modifications",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    // "editor.formatOnPaste": true,
    // "editor.formatOnType": true,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.defaultProfile.osx": "bash",
    "vsicons.dontShowNewVersionMessage": true,
    "cSpell.userWords": [
        "Suneli"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
   
    "[css]": {
        "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "tabnine.experimentalAutoImports": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "[jade]": {
    
        "diffEditor.ignoreTrimWhitespace": false
    },
}